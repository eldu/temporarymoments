{
  "name": "temporarymoments",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "develop": "bundle exec jekyll clean && run-p develop:{webpack,jekyll}",
    "develop:jekyll": "timeout 5 && bundle exec jekyll serve --watch",
    "develop:webpack": "webpack --mode development --watch",
    "build": "webpack --config webpack.config.js && bundle exec jekyll build",
    "deploy": "gh-pages-deploy"
  },
  "gh-pages-deploy": {
    "prep": [
      "build"
    ],
    "noprompt": true
  },
  "dependencies": {
    "photoswipe": "^4.1.3"
  },
  "devDependencies": {
    "@webpack-cli/init": "^1.0.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^5.0.1",
    "gh-pages-deploy": "^0.4.2",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
    "npm-cli": "^0.1.0",
    "npm-lifecycle": "^3.1.5",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  }
}
